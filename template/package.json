        "bundle:dev": "node ./nitro/local-build --bundle-dev",
        "bundle:prod": "node ./nitro/local-build --bundle-prod",
        "lint": "node ./nitro/local-build --lint",
        "test": "node ./nitro/local-build --test"
    },
    "ava": {
        "require": [
            "@babel/polyfill",
            "@babel/register",
            "ignore-styles"
        ]
